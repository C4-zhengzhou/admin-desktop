{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": { "node": 10 }
          }
        ]
      ]
    },
    "renderer": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false
          }
        ]
      ]
    },
    "web": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false
          }
        ]
      ]
    }
  },
  "plugins": ["@babel/transform-runtime"]
}
